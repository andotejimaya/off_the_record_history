{
  "name": "off-the-record History",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "off-the-record history",
  "permissions": [
    "history",
    "storage"
  ],  
  "background": {
    "scripts": ["js/background.js"]
  },
  "icons": {
    "128": "img/icon-128x128.png",
    "48": "img/icon-48x48.png",
    "16": "img/icon-16x16.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": false
  }
}
